{
  "name": "emd-monorepo",
  "packageManager": "pnpm@9.11.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @apps/store dev",
    "build": "pnpm -r --parallel build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r --if-present typecheck",
    "prepare": "husky",
    "dev:stripe-webhook": "node scripts/stripe-webhook-test-server.js",
    "dev:test:stripe": "tsx scripts/dev-test-stripe.ts",
    "sync:stripe-prices": "node scripts/sync-stripe-prices.mjs",
    "generate:products": "node scripts/generate-product-yamls.mjs",
    "fetch:ghl-products": "node scripts/fetch-ghl-products.mjs",
    "cleanup:stripe-products": "node scripts/cleanup-stripe-products.mjs",
    "stripe:create-prices": "node scripts/create-stripe-prices.mjs",
    "test:checkout": "node scripts/test-checkout-sessions.mjs",
    "test:perf": "node scripts/lighthouse-budget.mjs",
    "sync:shopify:products": "node scripts/sync-shopify-products.mjs",
    "merchant:upload": "tsx scripts/google-merchant/upload-products.ts",
    "merchant:upload:store": "GOOGLE_MERCHANT_ACCOUNT_ID=5670488999 GOOGLE_SERVICE_ACCOUNT_KEY_PATH=google-creds.json tsx scripts/google-merchant/upload-products.ts",
    "merchant:export": "tsx scripts/google-merchant/export-feed.ts",
    "merchant:export:store": "GOOGLE_MERCHANT_SITE_URL=https://apps.serp.co GOOGLE_MERCHANT_APPS_URL=https://apps.serp.co tsx scripts/google-merchant/export-feed.ts",
    "setup:shopify:metafields": "node scripts/setup-shopify-metafields.mjs",
    "validate:products": "pnpm --filter @apps/store validate:products",
    "update:video-metadata": "pnpm --filter @apps/store update:video-metadata",
    "test:lhci:local": "pnpm --filter @apps/store test:lhci:local",
    "test:unit": "pnpm --filter @apps/store test:unit",
    "test:all": "pnpm --filter @apps/store test:all",
    "test:smoke": "pnpm --filter @apps/store test:smoke",
    "ci:unit": "pnpm --filter @apps/store test:unit && pnpm --filter @apps/store validate:products"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-switch": "^1.2.6",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.18.2",
    "jsdom": "^24.1.3",
    "next-themes": "^0.3.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.4",
    "rehype-pretty-code": "^0.13.2",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "stripe": "^15.12.0",
    "unified": "^11.0.5",
    "vaul": "^0.9.9",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "globals": "^16.4.0",
    "husky": "^9.0.0",
    "tsx": "^4.20.6"
  }
}
